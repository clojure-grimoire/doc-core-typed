{:ns "clojure.core.typed.macros", :name "when-let-fail", :file "clojure/core/typed/macros.clj", :type :macro, :src "(defmacro when-let-fail \n  \"Like when-let, but fails if the binding yields a false value.\"\n  [b & body]\n  `(if-let ~b\n     (do ~@body)\n     (throw (ex-info (str \"Expression was nil or false\") {:form '~(second b)}))))", :column 1, :line 191, :macro true, :arglists ([b & body]), :doc "Like when-let, but fails if the binding yields a false value."}